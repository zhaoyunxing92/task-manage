<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sunny.task.mapper.project.ProjectVersionMapper">
    <resultMap id="BaseResultMap" type="com.sunny.task.model.project.ProjectVersion">
        <id column="ID" property="id" jdbcType="BIGINT"/>
        <result column="UNIQUE_ID" property="uId" jdbcType="VARCHAR"/>
        <result column="PRO_ID" property="proId" jdbcType="VARCHAR"/>
        <result column="NAME" property="name" jdbcType="VARCHAR"/>
        <result column="INTRO" property="intro" jdbcType="VARCHAR"/>
        <result column="CREATOR" property="creator" jdbcType="BIGINT"/>
        <result column="CREATION_DATE" property="creationDate" jdbcType="TIMESTAMP"/>
        <result column="MODIFIER" property="modifier" jdbcType="BIGINT"/>
        <result column="MODIFICATION_DATE" property="modificationDate" jdbcType="TIMESTAMP"/>
    </resultMap>
    <sql id="Base_Column_List">
    ID, UNIQUE_ID, PRO_ID, NAME, INTRO, CREATOR, CREATION_DATE, MODIFIER, MODIFICATION_DATE
  </sql>
    <select id="selectByPrimaryKey" resultMap="BaseResultMap" parameterType="java.lang.Long">
        select
        <include refid="Base_Column_List"/>
        from project_version
        where ID = #{id,jdbcType=BIGINT}
    </select>
    <delete id="deleteByPrimaryKey" parameterType="java.lang.Long">
    delete from project_version
    where ID = #{id,jdbcType=BIGINT}
  </delete>

    <insert id="insertSelective" parameterType="com.sunny.task.model.project.ProjectVersion">
        <selectKey resultType="java.lang.Long" keyProperty="id" order="AFTER">
            SELECT LAST_INSERT_ID()
        </selectKey>
        insert into project_version
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="uId != null">
                UNIQUE_ID,
            </if>
            <if test="proId != null">
                PRO_ID,
            </if>
            <if test="name != null">
                NAME,
            </if>
            <if test="intro != null">
                INTRO,
            </if>
            <if test="creator != null">
                CREATOR,
            </if>
            <if test="creationDate != null">
                CREATION_DATE,
            </if>
            <if test="modifier != null">
                MODIFIER,
            </if>
            <if test="modificationDate != null">
                MODIFICATION_DATE,
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="uId != null">
                #{uId,jdbcType=VARCHAR},
            </if>
            <if test="proId != null">
                #{proId,jdbcType=VARCHAR},
            </if>
            <if test="name != null">
                #{name,jdbcType=VARCHAR},
            </if>
            <if test="intro != null">
                #{intro,jdbcType=VARCHAR},
            </if>
            <if test="creator != null">
                #{creator,jdbcType=BIGINT},
            </if>
            <if test="creationDate != null">
                #{creationDate,jdbcType=TIMESTAMP},
            </if>
            <if test="modifier != null">
                #{modifier,jdbcType=BIGINT},
            </if>
            <if test="modificationDate != null">
                #{modificationDate,jdbcType=TIMESTAMP},
            </if>
        </trim>
    </insert>

    <!--修改项目版本信息-->
    <update id="updateByPrimaryKeySelective" parameterType="com.sunny.task.model.project.ProjectVersion">
        update project_version
        <set>
            <if test="name != null">
                NAME = #{name,jdbcType=VARCHAR},
            </if>
            <if test="intro != null">
                INTRO = #{intro,jdbcType=VARCHAR},
            </if>
            <if test="modifier != null">
                MODIFIER = #{modifier,jdbcType=BIGINT},
            </if>
            <if test="modificationDate != null">
                MODIFICATION_DATE = #{modificationDate,jdbcType=TIMESTAMP},
            </if>
        </set>
        where UNIQUE_ID = #{uId,jdbcType=VARCHAR}
    </update>

    <!--删除项目版本-->
    <delete id="deleteProjectVersion">
    delete from project_version
    where  UNIQUE_ID = #{uId,jdbcType=VARCHAR}
  </delete>

    <!--获取项目版本-->
    <select id="selectProjectVersionsByProId" resultMap="BaseResultMap">
          select
            UNIQUE_ID, PRO_ID, NAME, INTRO, CREATION_DATE
          from project_version
          where PRO_ID =  #{proId,jdbcType=VARCHAR}
    </select>
</mapper>
